#include <iostream>
using namespace std;
bool prime(int n)
{
	if(n<=1)return false;
	else if(n<=3)return true;
	else if(n%2==0 || n%3==0)return false;
	else
	{
		for(int i=5;i*i<=n;i+=6)
		{
			if(n%i==0 || n%(i+2)==0)
			return false;
		}
	}
	return true;
}
int main()
{	
	int n;
	cin>>n;
	int s=0;
	for(int i=1;;i++)
	{
			
		if(prime(i))
		{
			s++;
			if(s==n)
			{
				cout<<i<<endl;
				break;
			}
		}
	}
}
